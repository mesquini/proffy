    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.0.0",
    "knex": "^0.21.2",
    "morgan": "^1.10.0",
    "sqlite3": "^5.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/helmet": "^0.0.47",
    "@types/morgan": "^1.9.1",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  }
}
